{
    "name": "fratac/laravel-distributed-kv",
    "description": "Distributed key-value registry between Laravel applications using only PHP/Laravel.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fratac\\LaravelDistributedKv\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fratac\\LaravelDistributedKv\\LaravelDistributedKvServiceProvider"
            ],
            "aliases": {
                "DistributedKv": "Fratac\\LaravelDistributedKv\\Facades\\DistributedKv"
            }
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0"
    }
}
